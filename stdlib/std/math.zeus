
fn abs(a : i32, b : i32) : i32 {
    if a < b {
        return b - a;
    }
     return a - b;
}
fn max(a : i32, b : i32) : i32 {
    if a > b {
        return a;
    }
    return b;
}

fn min(a : i32, b : i32) : i32 {
    if a < b {
        return a;
    }
    return b;
}
@extern(libname="m", funcname="sqrtf")
extern fn sqrtf(x: float) : float;

@extern(libname="m", funcname="sqrt")
extern fn sqrt(x: double) : double;
