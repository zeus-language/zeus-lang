
struct Vec2{
    x: float,
    y: float,

     fn multiply(self: &Vec2, other: Vec2) : Vec2 {
        return Vec2 { x: self.x * other.x, y: self.y * other.y };
     }

     fn print(self: &Vec2): void {
        println(self.x);
        println(self.y);
     }
}

fn main() : i32 {
    let v1: Vec2 = Vec2 { x: 2.0f, y: 3.0f };
    let v2: Vec2 = Vec2 { x: 4.0f, y: 5.0f };

    let v3: Vec2 = v1.multiply(v2);

    println("Result of multiplication:");
    v3.print();

    return 0;
}
