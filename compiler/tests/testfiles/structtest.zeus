
struct Vec2 {
    x: float,
    y: float,
}

fn add(a: Vec2, b: Vec2) : Vec2 {
    println(a.x);
    println(a.y);
    return Vec2 { x: a.x + b.x, y: a.y + b.y };
}
fn main() : i32 {
    let v1: Vec2 = Vec2 { x: 1.0, y: 2.0 };
    println("----V1----");
    println(v1.x);
    println(v1.y);
    let v2: Vec2 = Vec2 { x: 3.0, y: 4.0 };
    println("----V2----");
    println(v2.x);
    println(v2.y);
    let v3: Vec2 = add(v1, v2);
    println("Result:");
    println(v3.x);
    println(v3.y);
    println("----Multiply Manually----");
    let mul : Vec2;
    mul.x = v1.x * v2.x;
    mul.y = v1.y * v2.y;
    println(mul.x);
    println(mul.y);
    return 0;
}