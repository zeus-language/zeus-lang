use std::io;
use std::str;
let NUMBER_CONST: i32 = 42;
let STRING_CONST: [u8] = "Hello, World!";
fn main() : i32 {
    let mut output : string = "Number constant: ${NUMBER_CONST}";
    println(output.as_c_str());
    let mut output2 :string = "String constant: ${STRING_CONST}";
    println(output2.as_c_str());
    output.destroy();
    output2.destroy();
    return 0;
}