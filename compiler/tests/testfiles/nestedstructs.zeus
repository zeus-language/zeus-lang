struct Vec2 {
    pub x: float,
    pub y: float,
}

struct Vec3 {
    pub v2: Vec2,
    pub z: float,
}

fn main(): i32 {
    let vec2: Vec2 = Vec2 { x: 1.0f, y: 2.0f };
    let mut vec3: Vec3 = Vec3 { v2: vec2, z: 3.0f };

    vec3.z = 3.0f;


    println("Vec2:");
    println(vec2.x);
    println(vec2.y);
    println("Vec3:");
    println(vec3.v2.x);
    println(vec3.v2.y);
    println(vec3.z);

    vec3.v2.x = 4.0f;
    vec3.v2.y = 5.0f;
    vec3.z = 6.0f;

    println("(modified) Vec3:");
    println(vec3.v2.x);
    println(vec3.v2.y);
    println(vec3.z);
    let vec4: Vec2 = Vec2 { x: 11.0f, y: 12.0f };
    vec3.v2  = vec4;

    println("(copy) Vec3:");
    println(vec3.v2.x);
    println(vec3.v2.y);
    println(vec3.z);

    return 0;
}
