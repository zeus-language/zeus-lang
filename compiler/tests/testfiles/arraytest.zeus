
fn len(arr : [i32; 5]) : i32 {
    return 5;
}

fn main() : i32 {
    let arr : [i32; 5] = [10, 20, 30, 40, 50];
    let mut sum : i32 = 0;
    for let i in 0 .. arr.len() {
        sum = sum + arr[i];
    }
    println("Array elements:");
    for let val in arr {
        println(val);
    }
    println("Sum of array elements:");
    println(sum);
    return 0;
}