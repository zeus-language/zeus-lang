use core::libc;
struct Point  {
    x: i32,
    y: i32,
    z: i32,

    fn __add__(self: &Point, other: i32) : Point {
        let mut p : Point;
        p.x = self.x + other;
        p.y = self.y + other;
        p.z = self.z + other;
        return p;
    }

    fn __sub__(self: &Point, other: i32) : Point {
        return Point{ x: self.x - other, y: self.y - other , z: self.z - other };
    }

}


fn main() : i32 {
    let p : Point =  Point{ x: 10, y: 20, z: 30 };
    let resultPlus : Point = Point{ x: 10, y: 20,z: 10 } + 5;
    let resultMinus : Point = Point{ x: 10, y: 20 ,z: 10} - 4;

    printf(r"P: (%d, %d)\n", p.x, p.y);
    printf(r"Result Plus: (%d, %d, %d)\n", resultPlus.x, resultPlus.y, resultPlus.z);
    printf(r"Result Minus: (%d, %d, %d)\n", resultMinus.x, resultMinus.y, resultMinus.z);

    return 0;
}